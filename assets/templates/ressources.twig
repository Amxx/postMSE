<script type="text/javascript">
	$(function(){
		$(window).on("scroll", materialKit.checkScrollForTransparentNavbar);
		window_width = $(window).width();
		if (window_width >= 768){
			big_image = $(".wrapper > .header");
			$(window).on("scroll", materialKitDemo.checkScrollForParallax);
		}

		var map_contact_fr = L.map("map_contact_fr").setView([45.742997, 4.823842], 14);
		map_contact_fr.addLayer(new L.TileLayer("https://b.tile.openstreetmap.org/{z}/{x}/{y}.png", {minZoom: 6, maxZoom: 18, attribution: ""}));
		L.marker([45.7298016, 4.8271596]).bindPopup("ENS de Lyon").addTo(map_contact_fr);
		L.marker([45.7455890, 4.8260870]).bindPopup("Home").addTo(map_contact_fr);

		var map_work_argonne = L.map('map_work_argonne').setView([41.713763, -87.982914], 12);
		map_work_argonne.addLayer(new L.TileLayer('https://b.tile.openstreetmap.org/{z}/{x}/{y}.png', {minZoom: 6, maxZoom: 18, attribution: ''}));
		L.marker([41.717447, -87.983429]).bindPopup('MCS, Bldg 240').addTo(map_work_argonne);

		$(".col-reactive").each(function(idx, el){ postMSE.client.reactBlock(el); });
		$(".packery").packery();
		$("a.scroolTo").click(function(id){
			$("html,body").animate({scrollTop: $($(this).attr("href")).offset().top});
			return false;
		});
	});
</script>